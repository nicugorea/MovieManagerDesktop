            <AnchorPane>
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="IMDb ID" textOrigin="CENTER" AnchorPane.topAnchor="0.0" />
               </children>
            </AnchorPane>
            <TextField fx:id="IMDbIDField" styleClass="textField">
               <VBox.margin>
